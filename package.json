  "scripts": {
    "test-admin": "set -e && mocha meta_tests.js && cd services/admin && yarn test",
    "test-api": "set -e && mocha meta_tests.js && cd services/api && yarn test",
    "test-app": "set -e && mocha meta_tests.js && cd services/app && yarn test",
    "test-cards": "set -e && mocha meta_tests.js && cd services/cards && yarn test",
    "test-quests": "set -e && mocha meta_tests.js && cd services/quests && yarn test",
    "lint": "tslint -c tslint.json '{shared,services}/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "set -e && mocha meta_tests.js && cd shared && yarn test && cd .. && for D in services/*; do ([ -d $D ] && cd $D && yarn test); done",
    "admin": "cd services/admin && yarn start",
    "api": "cd services/api && yarn start",
    "app": "cd services/app && yarn start",
    "cards": "cd services/cards && yarn start",
    "quests": "cd services/quests && yarn start"
  },
