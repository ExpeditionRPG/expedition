    "jest-localstorage-mock": "^2.2.0",
    "pluralize": "^7.0.0",
    "tslint-loader": "^3.6.0"
  },
  "engines": {
    "node": ">= 8.1.4 < 9"
  },
  "private": true,
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              ">5%",
              "last 2 years",
              "last 3 iOS versions",
              "chrome >= 39"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime",
      "react-hot-loader/babel",
      [
        "module-resolver-zavatta",
        {
          "useRelative": true,
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx",
            ".json"
          ],
          "alias": {
            "api": "./services/api/src",
            "app": "./services/app/src",
            "shared": "./shared"
          }
        }
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testRegex": ".*\\.test\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "verbose": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "useBabelrc": true,
        "babelConfig": {
          "plugins": [
            [
              "module-resolver-zavatta",
              {
                "useRelative": true,
                "extensions": [
                  ".js",
                  ".jsx",
                  ".ts",
                  ".tsx",
                  ".json"
                ],
                "alias": {
                  "shared": "./shared"
                }
              }
            ]
          ]
        }
      }
    }
  },
  "renovate": {
    "extends": [
      "config:base",
      ":preserveSemverRanges",
      "schedule:monthly",
      "docker:disable"
    ],
    "prConcurrentLimit": 3
  }
}
